   <!--=================================
    Inner Header -->
    <section class="inner-header">
      <div class="container">
        <div class="row ">
          <div class="col-md-12">
            <ol class="breadcrumb">
              <li class="breadcrumb-item"><a href="/api/v1/album"><i class="fas fa-home me-1"></i>Gallery</a></li>
              <li class="breadcrumb-item active"><i class="fa-solid fa-chevron-right me-2"></i><span>{{gdata.gallery_title}}</span></li>
            </ol>
          </div>
        </div>
      </div>
    </section>
    <!--=================================
    Inner Header -->

    <!--=================================
    Blog -->
    <section class="space-ptb">
      <div class="container" style="max-width: 1130px;">
        <div class="row">
          <div class="col-lg-7 col-xl-8 blog-single">
            <div class="blog-post-info">
              <div class="blog-content pb-0">
               
                <div class="blog-post-title">
                  <h3 class="mb-0" style="font-size: 20px;"><a href="#">{{gdata.gallery_title}}</a></h3>
                </div>
                <div class="blog-post post-style-07 border-0 py-4 px-0">
                  <div class="blog-post-details">
                    <div class="blog-post-meta p-0">
                      <div class="blog-post-user">
                        <a href="#">by<img class="img-fluid" src="images/avatar/02.jpg" alt="">By NEH Report</a>
                      </div>
                      <div class="blog-post-time">
                        <a href="#"><i class="fa-solid fa-calendar-days"></i>{{substr "16" gdata.update_date}}</a>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="owl-carousel owl-loaded owl-drag arrow-styel-04" data-nav-dots="false" data-nav-arrow="true" data-items="1" data-xl-items="1" data-lg-items="1" data-md-items="1" data-sm-items="1" data-xs-items="1" data-xx-items="1" data-autoheight="true" data-autoplay="false">
                    

                     <!-- item-01 -->
                    {{#gdata.gallery_path}}
                                   
                                    
                    <div class="item">
                      <div class="sport-tournament">
                        <div class="tournament-details">
                          <div class="">
                          <img src="https://northeastherald.sfo3.digitaloceanspaces.com/{{this}}" style="width: 100%;">
                          </div>
                          
                           </div>       
                      </div>
                    </div>
{{/gdata.gallery_path}}
                  </div>
                
                {{!-- <div class="singlenews" style="text-align: justify;">
                        {{{singlenews.post_summary}}}
                        {{{singlenews.post_content}}}
                </div>                 --}}
                
                
                
                
              
                
              </div>
            </div>
          </div>
          <div class="col-lg-5 col-xl-4">
            <div class="sidebar mt-lg-0">
              <div class="widget post-widget">
              <h6 class="widget-title">Related Post</h6>
              <div class="news-tab">

                <div class="tab-content" id="pills-tabContent03">
                  <div class="tab-pane fade show active" id="pills-latest" role="tabpanel" aria-labelledby="pills-latest-tab" tabindex="0">

                    {{#each recent}}
                    <div class="blog-post post-style-04">
                      <div class="blog-image" style="width: 20%;"> <img class="img-fluid" src="{{post_image}}" style="" alt=""> </div>
                      <div class="blog-post-details">
                        <h6 class="blog-title" style="font-size: 15px;"><a href="/{{post_category}}/{{post_url}}">{{substr "40" post_name}}</a></h6>
                        <div class="blog-post-meta">
                          <div class="blog-post-time"> <a href="#"><i class="fa-solid fa-calendar-days"></i>{{substr "16" update_date}}</a> </div>
                        </div>
                      </div>
                    </div>
                    {{/each}}

                    
                  </div>
                  
                </div>
              </div>
            </div>
              
              <div class="widget">
                <h6 class="widget-title">Newsletter</h6>
                <div class="newsletter">
                  <i class="fa-solid fa-envelope-open-text"></i>
                  <p>Subscribe For Michael Bean News And Receive Daily Updates</p>
                  <div class="newsletter-box">
                    <input type="email" class="form-control" placeholder="E-Mail Address">
                  <button type="submit" class="btn btn-primary">Subscribe Now</button>
                  </div>
                </div>
              </div>
              
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--=================================
    Blog -->

